{
  "manifest_version": 2,
  "name": "PhotoScryber",
  "version": "1.0",

  "description": "Transcribelate an image from an url",
  "permissions": ["contextMenus"],

  "background":{
      "scripts":["background.js"],
      "persistent": false
  },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "images/promo.png",
    "32": "images/promo.png",
    "48": "images/promo.png",
    "128": "images/promo.png"
  }
}
